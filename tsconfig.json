{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "cs-ui": ["./src"],
      "cs-ui/*": ["./src/*", "*"],
      "@styles/*": ["./src/styles/*", "*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
